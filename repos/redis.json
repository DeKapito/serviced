{
    "Description": "Redis Service",
    "Name": "Redis",
    "Services": [
        {
            "Command": "/usr/sbin/redis-server", 
            "Context": {},
            "VolumeExports": [ {
              "Name": "log",
              "Path": "var/log"
            } ],
            "VolumeImports": [ {
              "Name": "log",
              "ResourcePath" : "redis",
              "ContainerPath" : "/var/redis",
	            "Owner" : "zenoss",
	            "Permission" : 511
            }],
            "Endpoints": [
                {
                    "Application": "redis",
                    "PortNumber": 6379,
                    "Protocol": "tcp",
                    "Purpose": "export"
                }
            ],
            "ImageId": "zenoss/redis",
            "Instances": {
                "min": 1
            },
            "Launch": "auto",
            "Name": "redis",
            "Tags": ["core", "daemon"],
            "Services": []
        }
    ]
}

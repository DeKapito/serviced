{
    "CPUCommitment": 2,
    "Command": "/usr/bin/python3 -m http.server 8080",
    "ConfigFiles": {
        "/opt/zenoss/etc/global.conf": {
            "Content": "# Generated by serviced\n{{range $k,$v:=contextFilter . \"global.conf.\"}}{{$k}} {{$v}}\n{{end}}",
            "Filename": "/opt/zenoss/etc/global.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "660"
        },
        "/opt/zenoss/etc/zeneventserver.conf": {
            "FileName": "/opt/zenoss/etc/zeneventserver.conf",
            "Owner": "zenoss:zenoss",
            "Permissions": "0600"
        },
        "/opt/zenoss/etc/zeneventserver/jetty/jetty.ini": {
            "FileName": "/opt/zenoss/etc/zeneventserver/jetty/jetty.ini",
            "Owner": "zenoss:zenoss",
            "Permissions": "0600"
        },
        "/opt/zenoss/etc/zeneventserver/jetty/start.config": {
            "FileName": "/opt/zenoss/etc/zeneventserver/jetty/start.config",
            "Owner": "zenoss:zenoss",
            "Permissions": "0600"
        },
        "/opt/zenoss/etc/zeneventserver/logback.xml": {
            "FileName": "/opt/zenoss/etc/zeneventserver/logback.xml",
            "Owner": "zenoss:zenoss",
            "Permissions": "0600"
        }
    },
    "Description": "Stores and retrieves events from its database",
    "Endpoints": [
        {
            "Application": "zodb_.*",
            "Name": "mariadb",
            "Purpose": "import"
        },
        {
            "Application": "memcached",
            "Name": "memcached",
            "PortNumber": 11211,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "redis",
            "Name": "redis",
            "PortNumber": 6379,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "rabbitmq.*",
            "Name": "rabbitmq",
            "Purpose": "import"
        },
        {
            "Application": "zproxy",
            "Name": "zproxy",
            "PortNumber": 8080,
            "Protocol": "tcp",
            "Purpose": "import"
        },
        {
            "Application": "zep",
            "Name": "zep",
            "PortNumber": 8084,
            "Protocol": "tcp",
            "Purpose": "export"
        },
        {
            "Application": "controlplane_consumer",
            "Name": "controlplane_consumer",
            "PortNumber": 8444,
            "Protocol": "tcp",
            "Purpose": "import"
        }
    ],
    "HealthChecks": {
        "answering": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "mariadb_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "memcached_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "rabbit_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        },
        "redis_answering": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "running": {
            "Interval": 5.0,
            "Script": "echo OK"
        },
        "zep_answering": {
            "Interval": 10.0,
            "Script": "echo OK"
        }
    },
    "ImageID": "zenoss/ubuntu:wget",
    "Instances": {
        "min": 1
    },
    "Launch": "auto",
    "LogConfigs": [
        {
            "filters": [
                "zeneventserver"
            ],
            "path": "/opt/zenoss/log/zeneventserver.log",
            "type": "zeneventserver"
        },
        {
            "path": "/opt/zenoss/log/zeneventserver-stdio*.log",
            "type": "zeneventserver_stdio"
        }
    ],
    "Name": "zeneventserver",
    "Prereqs": [
        {
            "Name": "MariaDB connectivity",
            "Script": "su - zenoss -c '/opt/zenoss/bin/python /opt/zenoss/Products/ZenUtils/ZenDB.py --usedb zep --execsql=\";\"'"
        }
    ],
    "RAMCommitment": "1G",
    "Tags": [
        "daemon"
    ],
    "Volumes": [
        {
            "#####": "drwxrwxr-x 4 zenoss zenoss 4096 Jul 24 08:17 /opt/zenoss/var/zeneventserver",
            "ContainerPath": "/opt/zenoss/var/zeneventserver",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "zeneventserver"
        },
        {
            "ContainerPath": "/home/zenoss/.ssh",
            "Owner": "zenoss:zenoss",
            "Permission": "0755",
            "ResourcePath": ".ssh",
            "Type": ""
        },
        {
            "ContainerPath": "/var/zenoss",
            "Owner": "zenoss:zenoss",
            "Permission": "0775",
            "ResourcePath": "var-zenpacks"
        }
    ]
}